{
  "name": "Floos",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build:preparation": "node build-preparation.js",
    "android:clean": "cd android && ./gradlew clean",
    "android:prebuild": "rm -rf android/build && rm -rf android/app/build",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:clean": "watchman watch-del-all && react-native start --reset-cache",
    "init:template": "yarn install && react-native init $PROJECT_NAME && mv ./$PROJECT_NAME/android ./ && mv ./$PROJECT_NAME/ios ./ && rm -rf ./$PROJECT_NAME && node ./init_template_scripts/init_template_scripts && cd ios && pod install",
    "test": "jest",
    "lint": "eslint 'src/**/*' --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint 'src/**/*' --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "e2e:prepare": "node e2e/e2ePrepare.js",
    "e2e:reset": "node e2e/e2eReset.js",
    "e2e:build:ios": "detox build -c ios:release",
    "e2e:test:ios": "detox test -c ios:release -o e2e/config.json -u",
    "e2e:ios": "npm run e2e:prepare && npm run e2e:build:ios && npm run e2e:test:ios && npm run e2e:reset"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-native-firebase/app": "^12.9.0",
    "@react-native-firebase/dynamic-links": "^12.9.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-navigation/stack": "^6.0.7",
    "@sentry/react-native": "^3.1.0",
    "@stryberventures/stryber-react-native-ui-components": "^0.0.64",
    "@types/lodash": "^4.14.173",
    "axios": "^0.21.4",
    "immer": "^9.0.6",
    "lodash": "^4.17.21",
    "moment": "^2.8.4",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-bouncy-checkbox": "^2.1.7",
    "react-native-confirmation-code-field": "^7.1.0",
    "react-native-date-picker": "^4.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-inappbrowser-reborn": "3.6.3",
    "react-native-input-scroll-view": "^1.11.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^13.0.0",
    "react-native-reanimated": "^2.3.0-beta.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-webview": "^11.13.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "redux-saga-testing": "^2.0.1",
    "rn-fetch-blob": "^0.12.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/runtime": "^7.15.4",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.2",
    "@types/react-native": "^0.65.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "17.0.1",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "^27.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^18.20.4",
    "eslint": "^8.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.7",
    "react-native-dotenv": "^3.2.0",
    "react-native-flipper": "^0.115.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-flipper": "^2.0.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
