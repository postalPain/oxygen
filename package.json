{
  "name": "Floos",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:prebuild": "rm -rf android/build && rm -rf android/app/build",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:clean": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "build:preparation": "node build-preparation.js",
    "e2e:prepare": "node e2e/e2ePrepare.js",
    "e2e:reset": "node e2e/e2eReset.js",
    "e2e:ios": "yarn run e2e:prepare && yarn run e2e:build:ios && yarn run e2e:test:ios && yarn run e2e:reset",
    "e2e:build:ios": "detox build -c ios:release",
    "e2e:test:ios": "yarn run e2e:test:ios:firstchunk && yarn run e2e:test:ios:secondchunk",
    "e2e:test:ios:firstchunk": "detox test -c ios:release -o e2e/chunk_one/config.json -u",
    "e2e:test:ios:secondchunk": "detox test -c ios:release -o e2e/chunk_two/config.json -u",
    "lint": "eslint 'src/**/*' --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint 'src/**/*' --quiet --fix",
    "firebase:setdebug:android": "adb shell setprop debug.firebase.analytics.app ch.stryberventures.floos",
    "firebase:unsetdebug:android": "adb shell setprop debug.firebase.analytics.app .none."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/datetimepicker": "^2.3.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.2",
    "@react-native-firebase/analytics": "^7.5.1",
    "@react-native-firebase/app": "^8.4.0",
    "@react-native-firebase/dynamic-links": "^7.5.1",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/native": "^5.1.7",
    "@react-navigation/stack": "^5.2.14",
    "@sentry/react-native": "^1.3.7",
    "@stryberventures/stryber-react-native-ui-components": "^0.0.61",
    "@types/lodash": "^4.14.150",
    "axios": "^0.19.2",
    "d3": "^5.16.0",
    "formik": "^2.1.4",
    "html-entities": "^1.3.1",
    "immer": "^6.0.9",
    "jest-circus": "^26.5.3",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "pusher-js": "^6.0.3",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-date-picker": "^3.2.3",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-gifted-chat": "^0.16.1",
    "react-native-input-scroll-view": "^1.11.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-native-sensitive-info": "^5.5.5",
    "react-native-snap-carousel": "^4.0.0-beta.6",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-webview": "^10.4.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "redux-saga": "^1.1.3",
    "redux-saga-testing": "^2.0.1",
    "reselect": "^4.0.0",
    "rn-fetch-blob": "^0.12.0",
    "urbanairship-react-native": "^8.1.0",
    "url": "^0.11.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.1.0",
    "@types/node": "^13.13.4",
    "@types/react-native": "^0.62.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "16.9.2",
    "@types/yup": "^0.29.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "detox": "^17.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-react-redux": "^3.0.3",
    "eslint-plugin-redux-saga": "^1.1.3",
    "jest": "^26.5.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
